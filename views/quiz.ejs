<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Start the Quiz!</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>

    <!-- CSS FILE -->
  </head>

  <body>
    <header>
      <%- include('partials/_header') %>
    </header>
    <div class="quiz_content">
      <div>
        <div>
          <% title %>
        </div>
        <% for(let question in questions) { %>
          <div>
            <input type="radio" id="question" name="question" value="<% question %>">
            <label>
              <% question %>
            </label>
          </div>
          <%}%>

            <div>
              <a href="/quiz/<% quiz_id -1 %>">Back</a>
              <% if (quizzes.length !== 10) {%>
              <a href="/quiz/<% quiz_id +1 %>">Next</a>
              <% else {%>
              <button type="submit" >Submit</button>
              }
            </div>
      </div>
      <div class="quiz_sidebar">
        <table>
          <% for(let quiz in quizzes) { %>
            <tr><td><% quiz.title%></td></tr>
            <%}%>
        </table>
      </div>
    </div>
    </body>
